<div class="main-btn-area">
      <a class="done-page-title">達成済み</a>
    </div>

    <div class="main-area dones">
      <div class="whishes-bord wrapper">
        <% @wishes.each do |wish| %>
          <div class="wish-panel">
            <div class="wish-panel__icon">
              <i class="fas fa-ellipsis-h"></i>
              <ul class="nav">
              <li class="nav__item"><%= link_to "詳細", done_path(wish), class: "nav__link" %></li>
              <li class="nav__item"><%= link_to "削除", done_path(wish), method: :delete, class:"nav__link" %></li>
              </ul>
            </div>
            <div class="wish-panel__item">
            <%= wish.title%>
            </div>
            <div class="wish-panel__deadline">
            <%= wish.deadline%>
            </div>
          </div>
        <% end %>
      </div>
    </div>
